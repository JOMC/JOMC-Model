            xmlns:jomc="http://jomc.org/model"
            targetNamespace="http://jomc.org/model"
            elementFormDefault="qualified"
            attributeFormDefault="unqualified">
  <xsd:annotation>
    <xsd:documentation xml:lang="en"><![CDATA[

